<select>
    <option>Select channel:</option>
    {{#each arrayObject.channelArray}}
        <option value="{{channelName}}" {{#if channelActive}}selected{{/if}}>#{{channelName}}</option>
    {{/each}}
</select>
<script>
$(document).ready(function(){
    $("select").change(function(){
        var channel = $(this).val();
        window.location = '/c/' + channel;
    });
});
</script>
